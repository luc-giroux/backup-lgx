    <DataSet Name="AccessControl">
      <Fields>
        <Field Name="Time">
          <DataField>Time</DataField>
          <rd:TypeName>System.DateTime</rd:TypeName>
        </Field>
        <Field Name="Direction">
          <DataField>Direction</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Zone">
          <DataField>Zone</DataField>
          <rd:TypeName>System.Double</rd:TypeName>
        </Field>
        <Field Name="DeviceName">
          <DataField>DeviceName</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="IndivNdx">
          <DataField>IndivNdx</DataField>
          <rd:TypeName>System.Int16</rd:TypeName>
        </Field>
        <Field Name="Name">
          <DataField>Name</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
        <Field Name="Entreprise">
          <DataField>Entreprise</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
      </Fields>
      <Query>
        <DataSourceName>AccessControl</DataSourceName>
        <CommandText>DECLARE @Company as CHAR(10)
DECLARE @ZONE as Int
SELECT @Company = NULL
SELECT @Zone = NULL
SELECT	B.EventTime As Time, 
(select EventText from EventTypes where EventTypeId = B.EventTypeNdx) as Direction,
	B.FloatValue as Zone, 
	B.DeviceName,
	B.IndivNdx,
	ISNULL(RTRIM(LastName),'Nom de famille')+', '+ISNULL(RTRIM(Firstname),'Prénom') As Name,
	ISNULL(RTRIM(Custom01),'Entreprise') As Entreprise
FROM Events as B
INNER JOIN Individuals AS I ON I.IndivID = B.IndivNdx
INNER JOIN IndivCustomData As C ON I.IndivID = C.IndivNdx
WHERE B.FloatValue &gt; 0
AND Custom01 = ISNULL(@Company,Custom01)
AND B.FloatValue = ISNULL(@Zone, B.FloatValue)
AND B.EventTypeNdx IN (14,16)
AND B.EventTime BETWEEN Getdate() - 1 and getdate()
ORDER By B.IndivNdx, EventTime</CommandText>
        <rd:UseGenericDesigner>true</rd:UseGenericDesigner>
      </Query>
    </DataSet>
