                <SelectCommand>
                  <DbCommand CommandType="Text" ModifiedByUser="false">
                    <CommandText>SELECT     H.EntryID AS HoursEntryID, H.Contract, H.RateAssignLocation AS AssignedLocation, H.RatePayCurrencyID AS PayCurrencyID, D.DisciplineCode, 
                      D.Description AS DisciplineDescription, H.CompanyID, H.EmployeeID, RTRIM(E.LastName) + ', ' + E.FirstName AS EmployeeName, H.Year, H.Week, 
                      H.PostedDate, H.Hours, CASE WHEN [H].[LabourInvoiceNo] IS NULL THEN 0 ELSE 1 END AS Invoiced, ISNULL(I.Closed, 0) AS InvoiceClosed, 
                      H.LabourCost, H.OtherOfficeSpace, H.OtherOfficeExpenses, H.OtherIT, H.OtherITCore, H.Adjustment
